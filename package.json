{
	"name": "my-mui6-app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"format": "yarn biome format --write ./src",
		"lint": "yarn biome lint --write ./src",
		"fix": "yarn run lint && yarn run format",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006",
